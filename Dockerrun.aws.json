{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "client",
			"image": "aniketsharma0211/multi-client",
			"hostname": "client",
			"essential": false,
			"memory": 128
		},
		{
			"name": "server",
			"image": "aniketsharma0211/multi-server",
			"hostname": "api",
			"essential": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image": "aniketsharma0211/multi-worker",
			"hostname": "worker",
			"essential": false,
			"memory": 128
		},
		{
			"name": "worker",
			"image": "aniketsharma0211/multi-worker",
			"hostname": "worker",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "aniketsharma0211/multi-nginx",
			"essential": true,
			"memory": 128,
			"portMappings":[
			  {
				"hostPort": 80,
				"containerPort": 80
			  }
			],
			"links": ["client", "server"]
		}
	]
}